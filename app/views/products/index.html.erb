<% if user_signed_in? %> 
  <% if current_user.admin? %>
    <%= render "partials_/product_admin" %>
  <% else %>
    <%= render "partials_/product_user" %>
  <% end %>
<% else %>
  <%= render "partials_/product_user" %>
<% end %>   
<%= will_paginate @product, renderer: BootstrapPagination::Rails %>